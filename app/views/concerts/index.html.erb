<header>
<h1>All Concerts</h1>
</header>

<ul>
	<% @concerts.each do |concert| %>
			<li><strong>Artist: <%=concert.artist%></strong><br>
				<strong>Venue:</strong> <%=concert.venue%><br>
				<strong>City:</strong> <%=concert.city%><br>
				<strong>Date:</strong> <%=concert.date%><br>
				<strong>Price:</strong> <%=concert.price%><br>
				<strong>Description:</strong> <%=concert.description%>
			</li>
		<% end %>
</ul>

<footer>
	<%= link_to "Home", "/" %>
</footer>