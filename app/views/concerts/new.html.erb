<h1>Add a new concert:</h1>

<%= form_for @concert do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist %>
	<br>
	<%= f.label :venue %>
	<%= f.text_field :venue %>
	<br>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br> 
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br>
	<%= f.label :price %>
	<%= f.text_field :price %>
	<br>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<br>
	<%= f.submit "Create" %>
<% end %>
<br>
<%= link_to "Home", "/" %>